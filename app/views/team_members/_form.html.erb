
<h2 style="margin-bottom:50px;">Participate in This Meeting</h2>
<%= bootstrap_form_for(team_member, layout: :horizontal) do |f| %>
  <% if team_member.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@team_member.errors.count, "error") %> prohibited this team_member from being saved:</h2>

      <ul>
      <% @team_member.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div>
    <div class="field">
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.text_field :email %>
    </div>
    <div class="field">
      <%= f.hidden_field :meeting_id, value: @meeting.id %>
    </div>
    <div class="field">
      <center><%= f.file_field :file %></center>
    </div>
    <div class="actions">
      </br>
      <%= f.submit "Upload Schedule" %>
    </div>
  </div>
<% end %>
